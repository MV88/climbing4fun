{
  "name": "climbing4fun",
  "description": "A website used for tracking progress and attempts on climbing routes",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "climbing",
    "falesia",
    "climbers"
  ],
  "author": "Matteo Velludini chronosoutoftime@gmail.com",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "fix": "npm run lint:js --fix",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "core-js": "^3.6.5",
    "http": "0.0.1-security",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.14.6",
    "object-assign": "^4.1.1",
    "vue-picture-input": "^2.1.6",
    "vue-upload-image": "^1.1.5",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-vue": "^7.2.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.2.1",
    "vue-jest": "^3.0.4"
  }
}
